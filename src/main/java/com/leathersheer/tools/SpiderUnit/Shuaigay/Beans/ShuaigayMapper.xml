<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.leathersheer.tools.SpiderUnit.Shuaigay.Beans.ShuaigayMapper">

    <resultMap id="socialgame" type="com.leathersheer.tools.SpiderUnit.Shuaigay.Beans.SociaGameBean">
        <result column="uid" property="uid" />
        <result column="url" property="url" />
        <result column="postTitle" property="postTitle" />
    </resultMap>

    <insert id="insertColumn" parameterType="com.leathersheer.tools.SpiderUnit.Shuaigay.Beans.SociaGameBean">
        insert into socialgame(uid,url,postTitle)
        values(#{uid},#{url},#{postTitle})
        on conflict do nothing
    </insert>


</mapper>